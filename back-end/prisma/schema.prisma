
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid()) 
  name      String 
  email     String @unique
  password  String

  @@map("user")
  // transfers Transfers[]
}

model Transfers {
  id          String  @id @default(uuid()) 
  // id_user   String 
  // user        User    @relation(fields: [id_user], references: [id])
  title       String
  description String
  amount      String

  @@map("transfers")
}